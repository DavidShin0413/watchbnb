<h1>
  <% @watches.each do |watch| %>
    <ul>
      <img src="<%= watch.img_url %>">
      <li> User: <%= watch.user_id %> </li>
      <li> Name: <%= watch.name %> </li>
      <li> Style: <%= watch.style %> </li>
      <li> Location: <%= watch.location %> </li>
      <li> Price: $<%= watch.price %> </li>
    </ul>
  <% end %>
</h1>
<% if policy(Watch).create? %>
  <%= link_to "New watch", new_watch_path %>
<% end %>
